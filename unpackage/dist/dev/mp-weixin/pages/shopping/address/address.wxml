<view class="container"><block wx:if="{{addressList.length>0}}"><view class="address-list"><block wx:for="{{addressList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="item" data-address-id="{{item.id}}" data-event-opts="{{[['tap',[['selectAddress',['$event']]]]]}}" bindtap="__e"><view class="l"><view class="name">{{item.userName}}</view><block wx:if="{{item.defaultFlag}}"><view class="default">默认</view></block></view><view class="c"><view class="mobile">{{item.telNumber}}</view><view class="address">{{item.fullRegion+item.detailInfo}}</view></view><view class="r"><image class="del" data-address-id="{{item.id}}" src="http://shop.beastiot.com/haoyaoshopvf/icon/address-edit-7fee7b0d63.png" data-event-opts="{{[['tap',[['addressAddOrUpdate',['$event']]]]]}}" catchtap="__e"></image></view></view></block></view></block><block wx:if="{{addressList.length<=0}}"><view class="empty-view"><image class="icon" src="http://shop.beastiot.com/haoyaoshopvf/icon/noAddress-26d570cefa.png"></image><text class="text">收货地址在哪里</text></view></block><view class="add-address" data-address-id="0" data-event-opts="{{[['tap',[['addressAddOrUpdate',['$event']]]]]}}" bindtap="__e">新建</view></view>