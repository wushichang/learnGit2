<view class="container"><view class="ts-mes"><text class="ca">长按可取消收藏!</text></view><view class="collect-list"><block wx:for="{{collectList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="item" data-index="{{index}}" data-event-opts="{{[['tap',[['openGoods',['$event']]]],['touchstart',[['touchStart',['$event']]]],['touchend',[['touchEnd',['$event']]]]]}}" bindtap="__e" bindtouchstart="__e" bindtouchend="__e"><image class="img" src="{{item.listPicUrl}}"></image><view class="info"><view class="name">{{item.name||''}}</view><view class="subtitle">{{item.goodsBrief||''}}</view><view class="price">{{"￥"+(item.retailPrice||'0.00')}}</view></view></view></block></view><block wx:if="{{collectList.length<=0}}"><view class="empty-view"><image class="icon" src="/static/images/allorder.png"></image><text class="text">无收藏信息</text></view></block></view>