<view class="container"><view class="ts-mes"><text class="ca">长按可删除足迹!</text></view><view class="footprint"><block wx:for="{{footprintList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="day-item" id="{{'day-item-'+item.id}}"><view class="day-hd">{{item.addTime}}</view><view class="day-list"><view class="item" data-footprint="{{item}}" data-idx="{{index}}" data-event-opts="{{[['touchstart',[['touchStart',['$event']]]],['touchend',[['touchEnd',['$event']]]],['tap',[['deleteItem',['$event']]]]]}}" bindtouchstart="__e" bindtouchend="__e" bindtap="__e"><image class="img" src="{{item.listPicUrl}}"></image><view class="info"><view class="name">{{item.name||''}}</view><view class="subtitle">{{item.goodsBrief||''}}</view><view class="price">{{"￥"+(item.retailPrice||'0.00')}}</view></view></view></view></view></block></view><block wx:if="{{footprintList.length<=0}}"><view class="empty-view"><image class="icon" src="/static/images/allorder.png"></image><text class="text">没有足迹信息</text></view></block></view>